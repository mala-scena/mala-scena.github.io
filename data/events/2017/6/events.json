[ { "title": "Ateliér Design oděvů – Look back / forward", "duration": "", "price": "", "start": {"date":"2017-06-01 00:00:00 +0000","time":"17:30"}, "end": {"date":"2017-06-27 00:00:00 +0000","time":"22:00"}, "category": "výtvar/no.621", "annotation": "<p>Absolventská výstava 2017. Ateliér designu a oděvu. SŠUP Ústí nad Orlicí.</p>\n", "image": "https://msuo-media.s3.eu-central-1.amazonaws.com/media/2017/06/atelier_design_odevu_–_look_back_|_forward.jpg", "url": "/akce/2017/06/atelier_design_odevu_%E2%80%93_look_back_%7C_forward" }, { "title": "Mladá scéna", "duration": "", "price": "", "start": {"date":"2017-06-21 00:00:00 +0000","time":"14:00"}, "end": {"date":"2017-06-25 00:00:00 +0000","time":"12:30"}, "category": "je/libo/divadlo", "annotation": "<ol start=\"15\">\n<li>celostátní přehlídka studentských divadelních souborů\n21.–25. června, Ústí nad Orlicí\nCelostátní přehlídku studentských divadelních souborů Mladá scéna 2017 pořádá z pověření a za finančního přispění Ministerstva kultury NIPOS-ARTAMA ve spolupráci s Malou scénou a Gymnáziem Ústí nad Orlicí za finančního přispění Pardubického kraje a města Ústí nad Orlicí.</li>\n</ol>\n", "image": "https://msuo-media.s3.eu-central-1.amazonaws.com/media/2017/06/mlada_scena.jpg", "url": "/akce/2017/06/mlada_scena" }, { "title": "Amordie, černý jako šmér", "duration": "", "price": "", "start": {"date":"2017-06-21 00:00:00 +0000","time":"14:00"}, "end": {"date":null,"time":null}, "category": "je/libo/divadlo", "annotation": "<p>Divadlo v kině, Rosice / 25‘ / Malá scéna</p>\n", "image": "https://msuo-media.s3.eu-central-1.amazonaws.com/media/2017/06/amordie,_cerny_jako_smer.jpg", "url": "/akce/2017/06/amordie,_cerny_jako_smer" }, { "title": "Andrew Coffey – vyprávěj příběh", "duration": "", "price": "", "start": {"date":"2017-06-22 00:00:00 +0000","time":"15:00"}, "end": {"date":null,"time":null}, "category": "je/libo/divadlo", "annotation": "<p>Divadlo Vydýcháno, ZUŠ Liberec, 20’, Malá scéna</p>\n", "image": "https