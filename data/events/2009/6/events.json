[ { "title": "Malý Tibet - Ladakh", "duration": "", "price": "", "start": {"date":"2009-05-31 00:00:00 +0000","time":"17:00"}, "end": {"date":"2009-06-30 00:00:00 +0000","time":"23:59"}, "category": "výtvar/no.621", "annotation": "<p>Výstava fotografií Jana Tilingera a Ondřeje Cundra z o.s. Surya, které v září 2008 úspěšně dokončilo stavbu Sluneční školy ve vesnici Kargyak v indickém Himálaji výšce 4200 m n.m.</p>\n", "image": "https://msuo-media.s3.eu-central-1.amazonaws.com/media/2009/05/maly_tibet_-_ladakh.jpg", "url": "/akce/2009/05/maly_tibet_-_ladakh" }, { "title": "Mladá scéna 2009", "duration": "", "price": "", "start": {"date":"2009-06-04 00:00:00 +0000","time":"09:00"}, "end": {"date":"2009-06-06 00:00:00 +0000","time":"00:00"}, "category": "je/libo/divadlo", "annotation": "<p>Celostátní přehlídka studentského divadla poprvé v Ústí nad Orlicí.</p>\n", "image": "https://msuo-media.s3.eu-central-1.amazonaws.com/media/2009/06/mlada_scena_2009.jpg", "url": "/akce/2009/06/mlada_scena_2009" }, { "title": "Dvě sestry", "duration": "45 min.", "price": "", "start": {"date":"2009-06-06 00:00:00 +0000","time":"14:45"}, "end": {"date":null,"time":null}, "category": "je/libo/divadlo", "annotation": "<p>Malá scéna, 45 min.\nDivadelní soubor EMANUELA KRUMPÁČE Rosice.</p>\n", "image": "https://msuo-media.s3.eu-central-1.amazonaws.com/media/2009/06/dve_sestry.jpg", "url": "/akce/2009/06/dve_sestry" }, { "title": "Holky Elky", "duration": "40 min.", "price": "", "start": {"date":"2009-06-05 00:00:00 +0000","time":"14:00"}, "end": {"date":null,"time":null}, "category": "je/libo/divadlo", "annotation": "<p>Malá scéna, 40 min.\nATRAKTOR ZUŠ Liberec. Je to komedie z prostředí modelingu. Co všechno se může stát než vás oslní blesk fotoaparátu?</p>\n", "image": "https://msuo-media.s3.eu-central-1.amazonaws.com/media/2009/06/holky_elky.jpg", "url": "/akce/2009/06/holky_elky" }, { "title": "Hra a komunikace", "duration": "", "price": "Vstupné dobrovolné", "start": {"date":"2009-06-